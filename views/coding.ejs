<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<link rel="shortcut icon" type="image/ico" href="favicon.ico">
	<title>Johnny Pacia</title>
	<link rel="stylesheet" type="text/css" href="/dist/semantic.min.css">
	<script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
	<link rel="stylesheet" type="text/css" href="style.css">
	<script src="/dist/semantic.min.js"></script>
	<script src="main.js"></script>
</head> 
<body>
<div class="ui secondary pointing menu">
  <a class="item" href="/music">
    Music
  </a>
  <a class="active item" href="/">
    Present
  </a>
  <a class="item" href="/coding">
    Coding
  </a>
  <div class="right menu">
    <a class="ui item">
      Johnny Pacia
    </a>
  </div>
</div>
</div>
<div class="ui unstackable items">
  <div class="item">
    <div id="sounds-container">
		<div id="global-controls">
			<button id="play-all" class="control-btn">Play All</button>
			<button id="pause-all" class="control-btn">Pause All</button>
			<button id="reset-all" class="control-btn">Reset All</button>
			<button id="fade-out" class="control-btn">Fade Out</button>
		</div>
		<div id="mute-buttons">
			<button id="mute-guitar" class="sound-btn">Mute Guitar</button>
			<button id="mute-piano" class="sound-btn">Mute Piano</button>
			<button id="mute-drums" class="sound-btn">Mute Drums</button>
			<button id="mute-bass" class="sound-btn">Mute Bass</button>
		</div>
		<div id="effects">
			<button id="overdrive-guitar" class="overdrive">Overdrive</button>	
		</div>
	</div>
  </div>
  <script src="howlser.js"></script>
  <script src="sounds.js"></script>
 </body>
</html>




